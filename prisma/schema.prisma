
generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "mongodb"
  url      = env("DATABASE_URL")
}


model Modmail {
  id        String    @id @default(auto()) @map("_id") @db.ObjectId
  userId    String    @unique
  threadId  String
  createdAt DateTime  @default(now())
  closedAt  DateTime?
  open      Boolean   @default(true)
}
