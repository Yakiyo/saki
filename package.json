{
  "name": "saki",
  "version": "1.0.1",
  "description": "Saki bot for Gimai Seikatsu Discord Server",
  "main": "dist/index.js",
  "scripts": {
    "prepare": "husky install && prisma generate",
    "build": "tsc",
    "dev": "ts-node --transpile-only src",
    "start": "node .",
    "prestart": "rm -rf dist/ && tsc",
    "deploy": "ts-node --transpile-only src/deploy.ts",
    "logs": "ts-node scripts/logs.ts",
    "prs:seed": "prisma db seed",
    "prs:gen": "prisma generate",
    "fix": "rome format --write ."
  },
  "prisma": {
    "seed": "ts-node scripts/seed.ts"
  },
  "keywords": [
    "gimai",
    "discord",
    "bot",
    "anime"
  ],
  "author": "Yakiyo#1206",
  "license": "MIT",
  "dependencies": {
    "@discordjs/builders": "^1.9.0",
    "@discordjs/opus": "^0.8.0",
    "@discordjs/voice": "^0.14.0",
    "@distube/yt-dlp": "^1.1.3",
    "@napi-rs/canvas": "^0.1.34",
    "@naval-base/ms": "^3.1.0",
    "@prisma/client": "^4.12.0",
    "discord.js": "^14.8.0",
    "distube": "^4.0.4",
    "dotenv": "^16.0.3",
    "ffmpeg-static": "^4.4.1",
    "libsodium-wrappers": "0.7.10",
    "pino": "^7.11.0",
    "sourcebin": "^5.0.0",
    "tslib": "^2.5.0",
    "undici": "^5.16.0"
  },
  "devDependencies": {
    "@types/node": "^18.11.18",
    "husky": "^8.0.3",
    "prisma": "^4.12.0",
    "rome": "^11.0.0",
    "ts-node": "^10.9.1",
    "typescript": "^4.9.5"
  }
}
